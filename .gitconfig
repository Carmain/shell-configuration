[alias]
    a = add
    all = add .
    out-all = checkout -- .
    c = commit
    amend = commit --amend
    d = diff
    cached = diff --cached
    st = status
    co = checkout
    br = branch
    tree = log --graph --oneline --all
    tag-date = for-each-ref --sort=-taggerdate --count=10 --format '%(tag) -- %(taggerdate:short)' refs/tags
    impact = diff --stat --color
